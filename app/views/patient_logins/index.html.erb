<p id="notice"><%= notice %></p>

<h1>Patient Logins</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellidos</th>
      <th>Fecha</th>
      <th>Sexo</th>
      <th>Especialidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_logins.each do |patient_login| %>
      <tr>
        <td><%= patient_login.Nombre %></td>
        <td><%= patient_login.Apellidos %></td>
        <td><%= patient_login.Fecha %></td>
        <td><%= patient_login.sexo %></td>
        <td><%= patient_login.Especialidad %></td>
        <td><%= link_to 'Show', patient_login %></td>
        <td><%= link_to 'Edit', edit_patient_login_path(patient_login) %></td>
        <td><%= link_to 'Destroy', patient_login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Login', new_patient_login_path %>
